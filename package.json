{
  "name": "devops-kit",
  "displayName": "devops-kit",
  "description": "开发项目必备工具集",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "vscodevim.vim",
    "golang.go",
    "oxc.oxc-vscode",
    "redhat.vscode-yaml",
    "alefragnani.project-manager",
    "drblury.protobuf-vsc",
    "eamodio.gitlens",
    "mhutchie.git-graph",
    "esbenp.prettier-vscode",
    "hashicorp.hcl",
    "hashicorp.terraform",
    "korekontrol.saltstack",
    "ms-vscode-remote.remote-ssh-edit",
    "ms-vscode-remote.remote-ssh",
    "ms-vscode.remote-explorer",
    "sanjulaganepola.github-local-actions",
    "signageos.signageos-vscode-sops"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devops-kit.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}